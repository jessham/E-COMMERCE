    <%= f.label "Nome" %>
    <%= f.text_field :nome %>

    <%= f.label "Email" %>
    <%= f.text_field :email %>

    <%= f.label "Apelido" %>
    <%= f.text_field :apelido %>

    <%= f.label "CNPJ" %>
    <%= f.text_field :cnpj %>

