<h1>E-COMMERCE - Login</h1>

<%= form_tag '/sellerlogin', :html => {} do %>
      				
    <%= flash[:notice] %>

	<br>
      			
	<%= text_field_tag :email, nil, :placeholder => "Email" %>
	      			
	<%= password_field_tag :password, nil, :placeholder => "Senha" %>
	      			
	<%= submit_tag "Login" %>
    
<% end %>

<%= link_to "Cadastre-se!", new_seller_path %>